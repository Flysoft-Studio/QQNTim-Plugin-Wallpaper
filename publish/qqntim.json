{
    "manifestVersion": "3.0",
    "id": "wallpaper",
    "name": "壁纸插件",
    "description": "自定义你的 QQNT 背景。",
    "version": "1.0.0",
    "author": "Flysoft",
    "injections": [
        {
            "type": "main",
            "script": "main.js"
        },
        {
            "type": "renderer",
            "stylesheet": "style.css",
            "script": "renderer.js"
        },
        {
            "type": "renderer",
            "page": ["settings"],
            "script": "settings.js"
        }
    ]
}
